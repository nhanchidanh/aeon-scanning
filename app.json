{
  "expo": {
    "name": "Aeon Scanning",
    "slug": "aeon-scanning",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo-app.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": false,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "This app uses the camera to recognize product expiration dates."
      },
      "bundleIdentifier": "com.mkcteam.aeonscanning"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mkc_team.aeonscanning"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "cameraPermission": "This app uses the camera to recognize product expiration dates."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "6b12feeb-b356-4f4e-9edd-9f167eed4170"
      }
    },
    "owner": "mkc_team",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/6b12feeb-b356-4f4e-9edd-9f167eed4170",
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0
    }
  }
}
